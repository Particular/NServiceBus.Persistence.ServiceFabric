<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <PlatformTarget>x64</PlatformTarget>    
    <TargetFramework>net452</TargetFramework>
    <AssemblyName>NServiceBus.Persistence.TestRunner</AssemblyName>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <DebugType>full</DebugType>
    <DebugSymbols>True</DebugSymbols>
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <DocumentationFile>$(ProjectDir)bin\$(Configuration)\$(AssemblyName).xml</DocumentationFile>
    <UpdateAssemblyInfo>true</UpdateAssemblyInfo>
    <NoWarn>1591, 0618</NoWarn>    
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.ServiceFabric" Version="5.6.210" />
    <PackageReference Include="Microsoft.ServiceFabric.Data" Version="2.6.210"/>
    <PackageReference Include="Microsoft.ServiceFabric.FabricTransport.Internal" Version="2.6.210"/>
    <PackageReference Include="Microsoft.ServiceFabric.Services" Version="2.6.210"/>    
    <PackageReference Include="Microsoft.ServiceFabric.Services.Remoting" Version="2.6.210"/>        
    <PackageReference Include="NUnit" Version="3.7.1"/>            
    <PackageReference Include="NUnitLite" Version="3.7.1"/>                
  </ItemGroup>  
</Project>